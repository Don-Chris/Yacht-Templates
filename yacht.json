        "image": "b4bz/homer:latest",
        "categories": [
            "website"
        ],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "network_mode": "host",
        "volumes": [
            {
                "container": "/www/assets",
                "bind": "/home/chris/.config/Homer/assets"
            }
        ],
        "env": [
            {
                "name": "PUID",
                "label": "PUID",
                "default": "1000"
            },
            {
                "name": "SUBFOLDER",
                "label": "SUBFOLDER",
                "default": "/links"
            },
            {
                "name": "PORT",
                "label": "PORT",
                "default": "8081"
            },
            {
                "name": "PGID",
                "label": "PGID",
                "default": "1000"
            }
        ]
    },
    {
        "type": 1,
        "title": "Yacht",
